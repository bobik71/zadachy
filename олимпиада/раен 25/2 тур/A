n = int(input())
a = [0]*(n+1)
for i in range(1,n+1):
    a[i]+=i
    if i % 2 == 0 and i % 3 == 0:
        a[i] += 5
    elif i % 2 ==0:
        a[i] += 3
    elif i % 3 ==0:
        a[i] += 2
s = 0
m = a[0]
for i in range(1,n+1):
    if a[i] > m:
        s += 1
        m = a[i]
print(a)
print(s)  

