t = int(input())
for i in range(t):
    n = int(input())
    a = list(map(int,input().split()))
    s1 = 0
    s2 = 0
    for i in a:
        if i == 1:
            s1+=1
        if i == 2:
            s2+=1
    print(s1,s2)
    if s1 % 2 == 0 and s2 % 2 == 0:
        print('YES')
    elif s2 % 2 == 1 and s1 % 2 == 0 and s1 > s2+2:
        print('YES')
    else:
        print('NO')
